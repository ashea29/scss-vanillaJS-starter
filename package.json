{
  "name": "scss-vanillajs-starter",
  "version": "1.0.0",
  "description": "SCSS Vanilla JS Starter",
  "scripts": {
    "build": "node ./nodeScripts/build.js",
    "sass-dev": "sass --watch --style=expanded --no-source-map scss:dist/css/styles.css",
    "sass-prod": "sass --watch --style=compressed --no-source-map scss:dist/css/styles.css",
    "dev": "nodemon --watch ./pages --watch ./js ./nodeScripts/build.js"
  },
  "author": "Andrew Shea",
  "license": "GPL-3.0",
  "dependencies": {
    "commander": "latest",
    "concurrently": "latest",
    "live-server": "^1.1.0",
    "nodemon": "latest",
    "sass": "^1.54.5"
  }
}