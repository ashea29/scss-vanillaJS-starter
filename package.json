{
  "name": "scss-vanillajs-starter",
  "version": "1.0.0",
  "description": "SCSS Vanilla JS Starter",
  "scripts": {
    "build": "rm -rf ./dist && concurrently \"node ./scripts/outputHTMLandJS.js\" \"node ./scripts/compileSass.js -P \"",
    "build-alt": "rm -rf ./dist && node ./scripts/build.js -P",
    "dev": "rm -rf ./dist && concurrently \"nodemon ./scripts/outputHTMLandJS.js\" \"node ./scripts/compileSass.js -D \" \"node ./scripts/serve.js\""
  },
  "author": "Andrew Shea",
  "license": "GPL-3.0",
  "dependencies": {
    "@minify-html/node": "^0.10.0",
    "commander": "latest",
    "concurrently": "latest",
    "live-server": "^1.2.2",
    "nodemon": "latest",
    "sass": "^1.55.0",
    "uglify-js": "^3.17.1"
  }
}
